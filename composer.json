{
    "name": "wasyl144/breeze-sanctum",
    "description": "Minimal Laravel authentication scaffolding with Blade and Tailwind or API",
    "keywords": ["laravel", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Wasyl144/breeze/issues",
        "source": "https://github.com/Wasyl144/breeze"
    },
    "authors": [
        {
            "name": "Micha≈Ç Frankiewicz",
            "email": "michal.frankiewicz@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^8.42|^9.0",
        "illuminate/support": "^8.42|^9.0",
        "illuminate/validation": "^8.42|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Wasyl144\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Wasyl144\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
